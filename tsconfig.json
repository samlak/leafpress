{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/_document.tsx",
    "pages/index.jsx",
    "pages/_app.jsx",
    "components/landing-page1.tsx",
    "components/landing-page2.jsx",
    "components/old-homepage/navbar.jsx",
    "pages/register/index.jsxjsx",
    "lib/utils.ts",
    "pages/api/subscription/upgrade.js",
    "lib/lemonsqueezy.js"
, "pages/z.jsx"  ],
  "exclude": ["node_modules"]
}
